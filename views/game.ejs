<!DOCTYPE html>
<html>
<head>
    <title>Socket.IO chat</title>
    <meta charset="utf-8" />
    <% include ./partials/header %>
</head>

<body>
    <div id="game-background">
        <!-- Ecran Ketchup -->
        <div class="row h-50 test-container mt-2" id="team-ketchup">
            <div class="col">
                <div class="container d-flex h-100">
                    <div class="row h-100 row justify-content-center align-self-center">
                        <div class="col">
                            <div data-points="10" id="head-bread-k" class="empty-bread mt-2"></div>
                            <div data-points="9" id="head-tomato-k" class="empty-tomato mt-2"></div>
                            <div data-points="8" id="head-meat-k" class="empty-meat mt-2"></div>
                            <div data-points="7" id="cheese-k" class="empty-cheese mt-2"></div>
