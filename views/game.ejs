                    <div data-points="19" id="frie-1-k" class="empty-frie m-2"></div>
                </div>
                <div data-points="18" id="bucket-k" class="empty-bucket"></div>
            </div>
            <div class="col">
                <!-- Section points -->
                <div class="row">
                    <div class="point-ketchup mr-2 d-flex justify-content-center">
                        <p id="point-ketchup-decade">0</p>
                    </div>
                    <div class="point-ketchup mr-2 d-flex justify-content-center">
                        <p id="point-ketchup-unit">0</p>
                    </div>
                </div>
            </div>
        </div>
